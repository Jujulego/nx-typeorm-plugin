{
  "$schema": "http://json-schema.org/schema",
  "cli": "nx",
  "title": "db-migrate executor",
  "description": "",
  "type": "object",
  "properties": {
    "database": {
      "type": "string",
      "description": "Database to migrate.",
      "default": "default"
    },
    "verbosity": {
      "alias": "v",
      "type": "string",
      "enum": ["debug", "info", "warn", "error"],
      "description": "Verbosity level.",
      "default": "info"
    }
  },
  "required": ["database"]
}
